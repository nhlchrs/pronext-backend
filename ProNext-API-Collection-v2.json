          "name": "Subscribe to Payout Updates",
          "request": {
            "method": "GET",
            "url": {
              "raw": "ws://{{baseUrl}}",
              "host": [
                "{{baseUrl}}"
              ]
            },
            "description": "// Subscribe to payout updates\nsocket.emit('subscribe_payout_updates', userId);\n\n// Listen for payout status changes\nsocket.on('payout_status', (payoutData) => {\n  console.log('Payout Status:', payoutData.status);\n});"
          }
        },
